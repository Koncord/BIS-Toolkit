  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/three": "^0.182.0",
    "esbuild": "^0.27.2",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "three": "^0.182.0"
  }
}
